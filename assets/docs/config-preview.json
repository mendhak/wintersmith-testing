{
  "output": "",
  "baseUrl": "/gpslogger/",
  "contents": "./src",
  "locals": {
    "url": "http://localhost:8080",
    "name": "GPSLogger for Android",
    "owner": "Mendhak",
    "description": "A battery efficient GPS logging application"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./plugins/blog.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 100
  }
}
